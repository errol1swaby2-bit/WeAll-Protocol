# web/.env.example

# App branding / labeling
VITE_WEALL_APP_NAME=WeAll
VITE_WEALL_ENV_LABEL=dev

# Prod safety: dev tools are OFF unless explicitly enabled
# Set to 1 only for local dev.
VITE_WEALL_ENABLE_DEV_TOOLS=0

# API base (can still be edited in UI)
VITE_WEALL_API_BASE=http://127.0.0.1:8000

# Client metadata (attached to requests)
VITE_WEALL_CLIENT_NAME=weall-web
VITE_WEALL_CLIENT_VERSION=0.1.0

# Cloudflare Turnstile
VITE_TURNSTILE_SITE_KEY=
